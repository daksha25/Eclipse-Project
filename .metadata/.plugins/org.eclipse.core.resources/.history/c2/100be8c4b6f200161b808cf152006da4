	public static void main(String[] args){
		int[] m=new int[10];
		int count=0;
		Scanner sc=new Scanner(System.in);
		for(int i=0;i<10;i++)
		{
			m[i]=sc.nextInt();
		}
		for(int k=0;k<10;k++)
		{
			for(int j=0;j<9;j++)
			{
		if(m[j]==m[j+1])
		{
			count++;
		}
			
		}	
		}
		System.out.println(count);
		
	}
}
